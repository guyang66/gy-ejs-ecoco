.tabs-wrap
  position relative
  overflow hidden
  width 100%
  height 50px
  background #FFFFFF
  box-shadow 0 1px 1.5px 0 #EBEDF6
  z-index 200
  .tabs-content
    overflow hidden
    position relative
    margin 0 auto
    width 1280px
    //display inline-block
    //left 50%
    //transform translateX(-50%)
    li
      position relative
      float left
      cursor pointer
      &.active
        a
          color #4169E1
        span
          opacity 1
      &:hover
        a
          color #4169E1
        span
          opacity 1
    a
      display block
      padding-right 50px
      font-size 16px
      line-height 50px
      color #7A8BA6
      transition all .3s
    span
      display block
      position absolute
      bottom 0
      width calc(100% - 50px)
      height 2px
      background-image linear-gradient(90deg, #1E90FF  0%, #4169E1 100%)
      text-indent -999
      opacity 0
      transition all .3s
