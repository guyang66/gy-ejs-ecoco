@keyframes slide-l {
  0% {
    transform: translateX(40%)
    opacity: 0.5
  }
  25% {
    transform: translateX(20%)
    opacity: 0.75
  }
  50% {
    transform: translateX(0)
    opacity: 1
  }
  75% {
    transform: translateX(-20%)
    opacity: 0.75
  }
  100% {
    transform: translateX(-40%)
    opacity: 0.5
  }
}

@keyframes slide-r {
  0% {
    transform: translateX(-40%)
    opacity: 0.5
  }
  25% {
    transform: translateX(-20%)
    opacity: 0.75
  }
  50% {
    transform: translateX(0)
    opacity: 1
  }
  75% {
    transform: translateX(20%)
    opacity: 0.75
  }
  100% {
    transform: translateX(40%)
    opacity: 0.5
  }
}
