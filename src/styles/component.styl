          .txt
            margin-top 12px
            transition all 0.5s ease-in-out
            opacity 0
            width 50%
            margin-left 50%
            transform translateX(-50%)
            text-align center
      .core-layout-fade-cell:hover
        background-color transparent
        .info-wrap
          svg
            line
              transition-delay: 0.1s;
              stroke #ffffff
            .top
              transform translateX(-1276px)
            .bottom
              transform translateX(1276px)
            .left
              transform translateY(600px)
            .right
              transform translateY(-600px)
            .top,.bottom
              stroke-dasharray: 638 638 638;
            .left,.right
              stroke-dasharray: 300 300 300;
          .icon-view
            margin-top 40px
            transition all 0.5s ease-in-out
